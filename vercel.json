{
  "version": 2,
  "framework": null,
  "functions":{
        "api/index.php": {
        "runtime": "vercel-php@0.7.0"
  } 
  },
 
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
    "build": {
    "env": {
      "PHP_VERSION": "8.2"
    }
  }
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_URL": "https://CRM.app",
    "DB_CONNECTION": "mysql",
    "DB_HOST": "db",
    "DB_PORT": "3306",
    "DB_DATABASE": "mydb",
    "DB_USERNAME": "root",
    "DB_PASSWORD": "password"
  }
}

  
